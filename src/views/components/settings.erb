<div id="settings-container" class="dnone">
  <div id="homeBar" onclick="home(settingsPage(false))">
    <i class="fas fa-home"></i>
    <h1> Settings </h1>
  </div>
  <div id="tile">
    <h2> Title Setting </h2>
    <input type="text" id="titleInput" name="title" class="input" placeholder="Title"></input>
    <button id="titleButton" class="button">Set</button>
  </div>
  <div id="tile">
    <h2> Favicon Setting </h2>
    <input type="text" id="faviconInput" name="favicon" class="input" placeholder="Favicon URL"></input>
    <button id="faviconButton" class="button">Set</button>
  </div>
  <div id="tile">
    <h2> Bare Server </h2>
    <input type="text" id="bareInput" name="bare" class="input" placeholder="Bare Server URL"></input>
    <button id="bareButton" class="button">Set</button>
  </div>
</div>
